@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">

    @if (ViewBag.Users != null)
    {
        <h3>Users in the tenant</h3>

        <ul>
            @foreach (var user in ViewBag.Users)
            {
                <li>@user</li>
            }
        </ul>
    }
    else
    {
        @ViewBag.Message
    }
</div>